package ex0416.el;

public class Product {
	private String code;
	private String name;
	private int qty;
	private int price;
	
	public String getCode() {
		return code;
	}
	public String getName() {
		return name;
	}
	public int getQty() {
		return qty;
	}
	public int getPrice() {
		return price;
	}
	
}
