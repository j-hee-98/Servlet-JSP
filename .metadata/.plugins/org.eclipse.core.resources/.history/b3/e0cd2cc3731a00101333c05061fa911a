package ex0416.el;

public class Product {
	private String code="A01";
	private String name="새우깡";
	private int qty=5;
	private int price=3500;
	
	public String getCode() {
		return code;
	}
	public String getName() {
		return name;
	}
	public int getQty() {
		return qty;
	}
	public int getPrice() {
		return price;
	}
	
}
